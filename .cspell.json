{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "language": "en-US",
  "ignorePaths": [
    "**/bin/*",
    "**/obj/*",
    "**/Upstream/*",
    "packages.lock.json",
    "PublicAPI.*.txt"
  ],
  "languageSettings": [
    {
      "languageId": "csharp",
      "words": ["bazz", "browsable", "nologo", "zyxwvutsrq"]
    },
    {
      "languageId": "csproj",
      "words": ["buildtransitive", "contentfiles", "linq", "visualstudio"]
    },
    {
      "languageId": "nuspec",
      "words": ["schweizer"]
    },
    {
      "languageId": "props",
      "words": ["analyzers", "linq"]
    }
  ],
  "overrides": [
    {
      "filename": "*.csproj",
      "languageId": "csproj"
    },
    {
      "filename": "*.nuspec",
      "languageId": "nuspec"
    },
    {
      "filename": "*.props",
      "languageId": "props"
    }
  ],
  "words": ["asbjornu", "teardowns", "thomaslevesque", "xunit"]
}
