{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "language": "en-US",
  "ignorePaths": ["**/Upstream/*", "PublicAPI.*.txt", "**/bin/*", "**/obj/*"],
  "languageSettings": [
    {
      "languageId": "csharp",
      "words": [
        "bazz",
        "browsable",
        "namespaceanddescendants",
        "netcoreapp",
        "nologo",
        "zyxwvutsrq"
      ]
    },
    {
      "languageId": "csproj",
      "words": ["ruleset", "visualstudio"]
    },
    {
      "languageId": "nuspec",
      "words": ["netstandard", "schweizer"]
    },
    {
      "languageId": "props",
      "words": ["analyzers", "netstandard"]
    }
  ],
  "overrides": [
    {
      "filename": "*.csproj",
      "languageId": "csproj"
    },
    {
      "filename": "*.nuspec",
      "languageId": "nuspec"
    },
    {
      "filename": "*.props",
      "languageId": "props"
    }
  ],
  "words": ["teardowns", "xunit"]
}
