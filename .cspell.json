{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "language": "en-GB",
  "ignorePaths": ["**/Upstream/*", "PublicAPI.*.txt", "**/bin/*", "**/obj/*"],
  "languageSettings": [
    {
      "languageId": "csharp",
      "words": [
        "bazz",
        "browsable",
        "namespaceanddescendants",
        "netcoreapp",
        "nologo",
        "zyxwvutsrq"
      ]
    },
    {
      "languageId": "csproj",
      "words": ["netcoreapp", "netstandard", "ruleset", "visualstudio"]
    },
    {
      "languageId": "markdown",
      "words": ["analyzers"]
    },
    {
      "languageId": "nuspec",
      "words": ["netstandard"]
    }
  ],
  "overrides": [
    {
      "filename": "{*.cmd,*.cs,*.csproj,*.ruleset}",
      "language": "en-US"
    },
    {
      "filename": "*.csproj",
      "languageId": "csproj"
    },
    {
      "filename": "*.nuspec",
      "languageId": "nuspec"
    }
  ],
  "words": ["Pakaski", "teardowns", "Vanja", "Xbehave", "xunit"]
}
